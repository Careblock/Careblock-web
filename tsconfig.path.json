{
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "react-jsx",
        "paths": {
            "@assets/*": ["/src/assets/*"],
            "@components/*": ["/src/components/*"],
            "@constants/*": ["/src/constants/*"],
            "@enums/*": ["/src/enums/*"],
            "@hooks/*": ["/src/hooks/*"],
            "@layouts/*": ["/src/layouts/*"],
            "@pages/*": ["/src/pages/*"],
            "@routes/*": ["/src/routes/*"],
            "@services/*": ["/src/services/*"],
            "@stores/*": ["/src/stores/*"],
            "@types/*": ["/src/types/*"],
            "@utils/*": ["/src/utils/*"],
            "@validations/*": ["/src/validations/*"]
        }
    }
}
